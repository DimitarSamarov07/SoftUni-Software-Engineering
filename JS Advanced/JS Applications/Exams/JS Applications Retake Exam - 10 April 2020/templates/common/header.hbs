<header id="header">
    <h1>MyBlog</h1>
    {{#if loggedIn}}
        <h2>Welcome <span>{{email}}!</span>!</h2>
    {{/if}}
    <nav id="nav">
        <ul>
            {{#if loggedIn}}
                <li><a href="#/logout">Logout</a></li>
            {{/if}}
