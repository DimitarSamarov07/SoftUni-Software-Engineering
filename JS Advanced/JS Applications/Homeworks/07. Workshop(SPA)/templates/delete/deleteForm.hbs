<form action="#/delete/{{objectId}}" id="deleteForm" method="POST">
    <label>Title</label>
    <input type="text" name="title" value="Us (2019)" disabled="">
    <label>Image Url</label>
    <input type="text" name="imageUrl"
           value="{{imageUrl}}"
           disabled="">
    <label>Description</label>
    <textarea type="text" name="description"
              disabled="">{{description}}</textarea>
    <label>Genres</label>
    <input type="text" name="genres" value="{{genres}}" disabled="">
    <label>Available Tickets</label>
    <input type="number" name="tickets" value="{{tickets}}" disabled="">
    <input type="submit" value="Delete">
</form>